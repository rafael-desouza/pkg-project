{
  "name": "pkg-project",
  "version": "1.0.0",
  "description": "A simple repo for test pkg features and configs",
  "bin": "./dist/index.js",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir dist --extensions \".ts\"",
    "package": "npm run build &&pkg . --targets node14-win-x64 --output ./exe/pkg-project"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafael-desouza/pkg-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rafael-desouza/pkg-project/issues"
  },
  "homepage": "https://github.com/rafael-desouza/pkg-project#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@babel/runtime-corejs3": "^7.23.9",
    "axios": "^1.6.7",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "reflect-metadata": "^0.2.1",
    "routing-controllers": "^0.10.4"
  },
  "pkg": {
    "assets": [
      "dist/api/**/*",
      "node_modules/@babel/runtime/helpers/**/*"
    ]
  }
}